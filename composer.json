{
    "name": "jshayes/fake-requests",
    "description": "A package to make it easier to test with guzzle.",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Hayes",
            "email": "justin@justinhayes.ca"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3|^7.2",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "orchestra/testbench": "^5.0",
        "php-coveralls/php-coveralls": "^2.0",
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "JSHayes\\FakeRequests\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\" : "tests/"
        }
    }
}
